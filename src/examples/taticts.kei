Rule type : Type.


Rule apply : (forall
               (_ : type)
               (H : (forall (a : type) -> Type))
               ->
               (H _)).

Rule rewrite : (forall
               (x : type)
               (y : type)
               (σ' : type)
               (H : (≡ x y))
               (H : (≡ x σ'))
               -> (≡ y σ')).
